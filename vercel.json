{
  "version": 2,
  "builds": [
    {
      "src": "dist/src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "bun run build",
  "installCommand": "bun run db:generate && bun run db:push &&  bun run start",
  "outputDirectory": "dist"
}
